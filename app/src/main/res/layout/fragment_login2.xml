<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="loginViewModel"
            type="com.example.mnt_android.viewmodel.LoginViewModel" />
        <variable
            name="loginActivity"
            type="com.example.mnt_android.view.ui.LoginActivity" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".view.ui.LoginActivity2">

        <TextView xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/tv_top_login2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            app:layout_constraintBottom_toTopOf="@id/iv_picture_login2"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@mipmap/ic_launcher_round" />

        <ImageView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/iv_picture_login2"
            android:layout_width="142dp"
            android:layout_height="157dp"
            app:layout_constraintBottom_toTopOf="@id/tv_name_login2"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_top_login2"
            app:srcCompat="@mipmap/ic_launcher_round" />

        <TextView xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/tv_name_login2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            app:layout_constraintBottom_toTopOf="@id/tv_id_login2"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/iv_picture_login2"
            app:layout_constraintVertical_bias="0.9" />

        <TextView xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/tv_id_login2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="카카오ID : "
            app:layout_constraintBottom_toTopOf="@id/bu_start_login2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_name_login2"
            app:layout_constraintVertical_bias="0.1" />

        <Button xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/bu_start_login2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Button"
            app:layout_constraintTop_toBottomOf="@id/tv_id_login2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"/>



    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>